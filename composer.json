{
  "name": "trl/tools-lifecycle-hub",
  "type": "project",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "laravel/sanctum": "^4.0",
    "maatwebsite/excel": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
